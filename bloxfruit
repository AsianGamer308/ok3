
local GUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/derekcool38/cats/main/UI-Librarys/ModernUI/SourceV2.lua"))()

local UI = GUI:CreateWindow("Blox Fruit Asian",{
    CornerRadius = 8; -- How Cornerius the ui is
    TitleSize = 14; -- The Size of the Title
    CustomFont = Enum.Font.Gotham; -- Custom Font for only the Title,The Discord Thing and the timer
    CustomParent = game.CoreGui; -- To choose where its located. CoreGui is best since its undetected in most games
    CustomPosition = UDim2.new(0.220079795, 0, 0.35324502, 0); --  Where you want the ui to "spawn"
})


local Main = UI:addPage("Main",{
    Visible = true
})
local PS = game:GetService("Players")

local function SetHumanoid(type,v)
    PS.LocalPlayer.Character.Humanoid[type] = v
end

local PlayerTable = {}
for i,v in pairs(game:GetService("Players"):GetPlayers()) do
   table.insert(PlayerTable,v.Name)
   table.sort(PlayerTable)
end

Main:addDropdown("TP To Player",PlayerTable,function(value)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[value].Character.HumanoidRootPart.CFrame
end)

Main:addToggle("Auto Click",function(value)
    getgenv().click = value
    while getgenv().click do
        game:GetService("VirtualUser"):ClickButton1(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        wait(.2)
    end
end)

Main:addToggle("Auto Chest1",function(value)
    getgenv().die = value
    while getgenv().die do
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Chest1.CFrame
        wait(1)
    end
end)

Main:addToggle("Auto Chest2",function(value)
    getgenv().die = value
    while getgenv().die do
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Chest2.CFrame
        wait(1)
    end
end)

Main:addToggle("Auto Chest3",function(value)
    getgenv().die = value
    while getgenv().die do
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Chest3.CFrame
        wait(1)
    end
end)

Main:addToggle("Auto Die Raid Map",function(value)
    getgenv().die = value
    while getgenv().die do
        for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
            if v ~= nil then
    v.Humanoid.Health = 0
            end
        end
        wait(0)
    end
end)

Main:addToggle("Auto Raider",function(value)
    getgenv().AutoFarm = value
    while getgenv().AutoFarm do
    game:GetService("VirtualUser"):ClickButton1(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    for _,v in pairs(game.Workspace.Enemies:GetDescendants()) do
    
    if string.find(v.Name, "Raid") and v:FindFirstChild("HumanoidRootPart") then
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame*CFrame.new(0,0,15)
    v:FindFirstChild("HumanoidRootPart").Size = Vector3.new(30,30,30)
    v:FindFirstChild("HumanoidRootPart").Transparency = .2
    end
    end
    wait()
    end
end)

Main:addToggle("Auto DeckHand",function(value)
    getgenv().AutoFarm = value
    while getgenv().AutoFarm do
    game:GetService("VirtualUser"):ClickButton1(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    for _,v in pairs(game.Workspace.Enemies:GetDescendants()) do
    
    if string.find(v.Name, "Ship Deck") and v:FindFirstChild("HumanoidRootPart") then
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame*CFrame.new(0,0,15)
    v:FindFirstChild("HumanoidRootPart").Size = Vector3.new(30,30,30)
    v:FindFirstChild("HumanoidRootPart").Transparency = .2
    end
    end
    wait()
    end
end)

Main:addToggle("Auto Artic",function(value)
    getgenv().AutoFarm = value
    while getgenv().AutoFarm do
    game:GetService("VirtualUser"):ClickButton1(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    for _,v in pairs(game.Workspace.Enemies:GetDescendants()) do
    if string.find(v.Name, "Arctic") and v:FindFirstChild("HumanoidRootPart") then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame*CFrame.new(0,0,15)
    v:FindFirstChild("HumanoidRootPart").Size = Vector3.new(30,30,30)
    v:FindFirstChild("HumanoidRootPart").Transparency = .2
    end
    end
    wait()
    end
 end)

Main:addToggle("Auto Snow",function(value)
    getgenv().AutoFarm = value
    while getgenv().AutoFarm do
    game:GetService("VirtualUser"):ClickButton1(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    for _,v in pairs(game.Workspace.Enemies:GetDescendants()) do
    if string.find(v.Name, "Snow") and v:FindFirstChild("HumanoidRootPart") then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame*CFrame.new(0,0,15)
    v:FindFirstChild("HumanoidRootPart").Size = Vector3.new(30,30,30)
    v:FindFirstChild("HumanoidRootPart").Transparency = .2
    end
    end
    wait()
    end
 end)

local Quest = UI:addPage("Quests")

Quest:addButton("Buy Fruit",function(value)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-456.966339, 73.0458984, 343.314362))
    wait(.1)
    local args = {
        [1] = "Candies",
        [2] = "Buy",
        [3] = 4,
        [4] = 1
    }
    
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end)

Quest:addButton("Get Raider",function(value)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-427.442719, 72.9963455, 1839.95032))
    wait(.1)
    local args = {
        [1] = "StartQuest",
        [2] = "Area1Quest",
        [3] = 1
    }
    
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end)

Quest:addButton("Get Deck",function(value)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(1031.20056, 125.082932, 32910.0273))
wait(.1)
local args = {
    [1] = "StartQuest",
    [2] = "ShipQuest1",
    [3] = 1
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end)

Quest:addButton("Get Artic",function(value)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(5667.56104, 31.5269318, -6482.71045))
    wait(.1)
    local args = {
        [1] = "StartQuest",
        [2] = "FrostQuest",
        [3] = 1
    }
    
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end)

Quest:addButton("Get Snow",function(value)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(5667.56104, 31.5269318, -6482.71045))
wait(.1)
local args = {
    [1] = "StartQuest",
    [2] = "FrostQuest",
    [3] = 2
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end)

