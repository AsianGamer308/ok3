local re = Library:Window("Rebirth")

re:Toggle("AutoRebirth", function(value)
    getgenv().rebirth = value
    while getgenv().rebirth do
    local args = {
    [1] = game:GetService("Players").LocalPlayer.leaderstats["\240\159\140\128Rebirth"]
        }
        game:GetService("ReplicatedStorage").RemoteEvents.Rebirth:FireServer(unpack(args))
    wait(.3)
end
end)


re:Button("Print Credits", function()
   print("Script By Asian")
   print("UI By Devyonte")
   print("bloodball")
end)
